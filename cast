env
ls /var ; ls /var
ls /hbtn ; ls /var
ls /var ; ls /hbtn
ls /var ; ls /hbtn ; ls /var ; ls /var
ls /var && ls /var
ls /hbtn && ls /var
ls /var && ls /var && ls /var && ls /hbtn
ls /var && ls /var && ls /var && ls /hbtn && ls /hbtn
ls /var || ls /var
ls /hbtn || ls /var
ls /hbtn || ls /hbtn || ls /hbtn || ls /var
ls /hbtn || ls /hbtn || ls /hbtn || ls /var || ls /var
